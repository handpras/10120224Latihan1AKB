<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    xml:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/frame"
        android:orientation="vertical"
        android:padding="@dimen/jarak_16dp">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/aqua_squeeze"
            app:cardCornerRadius="@dimen/jarak_8dp">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/logo"
                    android:layout_width="@dimen/jarak_48dp"
                    android:layout_height="@dimen/jarak_48dp"
                    android:layout_marginStart="@dimen/jarak_24dp"
                    android:layout_marginTop="23dp"
                    android:layout_marginBottom="@dimen/jarak_24dp"
                    android:background="@drawable/logo_1" />

                <TextView
                    android:id="@+id/tv_nama_logo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/jarak_10dp"
                    android:layout_marginTop="@dimen/jarak_16dp"
                    android:layout_marginBottom="@dimen/jarak_2dp"
                    android:layout_toEndOf="@+id/logo"
                    android:text="@string/digiteam"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_deskripsi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_nama_logo"
                    android:layout_marginStart="@dimen/jarak_10dp"
                    android:layout_marginTop="@dimen/jarak_2dp"
                    android:layout_marginBottom="@dimen/jarak_2dp"
                    android:layout_toEndOf="@+id/logo"
                    android:text="@string/tagline"
                    android:textColor="@color/boulder"
                    android:textSize="@dimen/text_size_12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_deskripsi"
                    android:layout_marginStart="@dimen/jarak_10dp"
                    android:layout_marginTop="@dimen/jarak_2dp"
                    android:layout_marginBottom="17dp"
                    android:layout_toEndOf="@+id/logo"
                    android:text="@string/jds"
                    android:textColor="@color/boulder"
                    android:textSize="@dimen/text_size_12sp" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/jarak_24dp"
            android:layout_marginBottom="28dp">

