                android:id="@+id/iv_elip"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/profile" />

            <TextView
                android:id="@+id/tv_nama_elip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/jarak_2dp"
                android:layout_marginStart="@dimen/jarak_8dp"
                android:layout_toEndOf="@+id/iv_elip"
                android:text="@string/name_person"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_16sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_nama_elip"
                android:layout_marginTop="@dimen/jarak_2dp"
                android:layout_marginStart="@dimen/jarak_8dp"
                android:layout_toEndOf="@+id/iv_elip"
                android:text="@string/jobdesk"
                android:textColor="@color/boulder"
                android:textSize="@dimen/text_size_14sp" />
        </RelativeLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="@dimen/jarak_8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical"
                android:padding="@dimen/jarak_16dp"
                >


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/title"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_20sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/jarak_16dp"

                    android:text="@string/subtitle"
                    android:textColor="@color/yale_blue"
                    android:textSize="@dimen/text_size_14sp"
                    android:textStyle="bold" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/jarak_8dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="@dimen/jarak_8dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/jam"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:background="@color/la_salle_green"
                                    android:padding="@dimen/jarak_8dp"
                                    android:text="@string/waktu"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/text_size_20sp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.5"
                                    android:background="@color/bluish_green"
                                    android:drawableRight="@drawable/icon_waktu"
                                    android:padding="8dp"
                                    android:textSize="20sp"
                                    android:textStyle="bold">

                                </TextView>


                            </LinearLayout>
                        </androidx.cardview.widget.CardView>


                    </RelativeLayout>
                </FrameLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/helper_text"
                    android:textColor="#151920"
                    android:textSize="@dimen/text_size_12sp" />

                <androidx.appcompat.widget.AppCompatButton

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:layout_marginBottom="24dp"
